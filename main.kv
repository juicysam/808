<MyScreenManager>:
    MainMenu:
        id: main_menu
    PlaylistMenu:
        id: playlist_menu
    LightsMenu:
        id: lights_menu
    EyesMenu:
        id: eyes_menu
    HiFiRushPlaylist:
        id: hifi_rush_playlist
    PsychPlaylist:
        id: psych_playlist
    MemePlaylist:
        id: meme_playlist


<MainMenu>:
    name: '_main_menu_'
    AnchorLayout:
        anchor_x: 'left'
        anchor_y: 'top'

        Button:
            text: 'Music-Sounds'
            color: [0,0,0]
            bold: False
            font_size: self.width / 10
            font_name: 'AVENGEANCE MIGHTIEST AVENGER'

            size_hint: 0.5,1/3

            background_normal: ''
            background_color: [255/255,75/255,80/255,1]

            on_press: root.manager.current = '_playlist_menu_'

    AnchorLayout:
        anchor_x: 'left'
        anchor_y: 'center'

        Button:
            text: 'Light Effects'
            color: [0,0,0]
            bold: False
            font_size: self.width / 10
            font_name: 'AVENGEANCE MIGHTIEST AVENGER'

            size_hint: 0.5,1/3

            background_normal: ''
            background_color: [255/255,255/255,129/255,1]

            on_press: root.manager.current = '_lights_menu_'

    AnchorLayout:
        anchor_x: 'left'
        anchor_y: 'bottom'

        Button:
            text: 'Eye Effects'
            color: [0,0,0]
            bold: False
            font_size: self.width / 10
            font_name: 'AVENGEANCE MIGHTIEST AVENGER'

            anchor_x: 'left'
            anchor_y: 'bottom'
            size_hint: 0.5,1/3

            background_normal: ''
            background_color: [94/255,174/255,201/255,1]

            on_press: root.manager.current = '_eyes_menu_'

    AnchorLayout:
        anchor_x: 'right'
        anchor_y: 'bottom'

        ToggleButton:
            text: 'Connect'
            color: [0,0,0]
            bold: False
            font_size: self.width / 10
            font_name: 'AVENGEANCE MIGHTIEST AVENGER'

            size_hint: 0.5,1/3

            background_normal: ''
            background_color: [255/255,75/255,80/255,1]

            on_press: root.toggle_connect()

<PlaylistMenu>:
    name: '_playlist_menu_'
    ScrollView:
        scroll_type:['bars', 'content']
        size_hint: (1,1)
        pos_hint: {"x":0, "top":1}
        do_scroll_x: False
        do_scroll_y: True
        BoxLayout:
            orientation: 'vertical'
            size_hint: (1, .9)
            height: self.minimum_height
            Button:
                text: 'Hi-Fi Rush Soundtrack'
                on_press: root.manager.current = '_hifi_rush_playlist_'
            Button:
                text: 'Psych The Musical'
                on_press: root.manager.current = '_psych_playlist_'
            Button:
                text: 'Meme Songs'
                on_press: root.manager.current = '_meme_playlist_'
    BoxLayout:
        orientation: 'horizontal'
        size_hint_y: .1
        #height: self.minimum_height
        Button:
            text: 'Back'
            on_press: root.manager.current = '_main_menu_'
        Button:
            text: 'Previous Song'
            on_press: root.write(2)
        Button:
            text: 'Pause'
            on_press: root.write(3)
        Button:
            text: 'Next Song'
            on_press: root.write(4)

